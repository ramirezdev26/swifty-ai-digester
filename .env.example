FRONTEND_URL=http://localhost:3000
API_PREFIX=/api

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_NAME=swifty_api

# Firebase Configuration
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour_Firebase_Private_Key_Here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email
FIREBASE_DATABASE_URL=https://your_project.firebaseio.com
FIREBASE_STORAGE_BUCKET=your_project.appspot.com

# Cloudinary Configuration (Required for image processing)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Gemini AI Configuration (Required for image processing)
GEMINI_API_KEY=your_gemini_api_key

# RabbitMQ Configuration
RABBITMQ_URL=amqp://user:pass@host/vhost

# RabbitMQ Processing Configuration (must match swifty-api)
RABBITMQ_EXCHANGE=pixpro.processing
RABBITMQ_DLX_EXCHANGE=pixpro.dlx
RABBITMQ_PARTITIONS=3
RABBITMQ_MESSAGE_TTL=300000
RABBITMQ_DLQ_TTL=86400000

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY_1=5000
RETRY_DELAY_2=15000
RETRY_DELAY_3=30000

# Processing Configuration
PROCESSING_TIMEOUT_MS=60000
PREFETCH_COUNT=1
